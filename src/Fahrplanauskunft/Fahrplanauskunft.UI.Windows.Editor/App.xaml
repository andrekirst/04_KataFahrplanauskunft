<Application x:Class="Fahrplanauskunft.UI.Windows.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Fahrplanauskunft.UI.Windows.Editor"
             StartupUri="FahrplanauskunftMainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Resource" Source="Resources\Resources.xaml" />
                <ResourceDictionary x:Name="Theme" Source="Themes\Default\Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
